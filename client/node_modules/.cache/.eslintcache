ata, setData] = useState([]);\r\n    const handleFile = async(e) => {\r\n        const file = e.target.files[0];\r\n        const data = await file.arrayBuffer();\r\n        const workbook = XLSX.readFile(data);\r\n        const worksheet = workbook.Sheets[workbook.SheetNames[3]];\r\n        const jsonData = XLSX.utils.sheet_to_json(worksheet, {header: 1, blankrows:false})\r\n        setData(jsonData)\r\n    }\r\n\r\n    const Hierarchies= [];\r\n    const HierarchyMap = new Map();\r\n    console.log(jsonData);\r\n    for(let data in jsonData){\r\n        if(data == 0){\r\n            continue;\r\n        }\r\n        if(HierarchyMap.has(jsonData[data][2])){\r\n            HierarchyMap.get(jsonData[data][2]).addItem(jsonData[data]);\r\n        }\r\n        else{\r\n            HierarchyMap.set(jsonData[data][2], new Hierarchy(jsonData[data], false));\r\n            Hierarchies.push(HierarchyMap.get(jsonData[data][2]));\r\n        }\r\n    }\r\n    console.log(Hierarchies)\r\n    console.log(HierarchyMap.size)\r\n\r\n    return(\r\n        <div className=\"component\">\r\n            <ul className=\"category-items\">\r\n                <li>Available Categories</li>\r\n                <li>Comment</li>\r\n                <li>Location</li>\r\n            </ul>\r\n            <input type=\"file\" onChange={(e) => handleFile(e)}/>\r\n            {Hierarchies.map((item, i) =>\r\n            <div key={i}>{item.returnHTML()}</div>)}\r\n        </div>\r\n    )\r\n}\r\n","C:\\Users\\Pete\\Desktop\\CS410_northwestern\\client\\src\\components\\Dropdown_Gen.js",["92","93"],[],"export default class Hierarchy{\r\n    constructor(arr = [], bool) {\r\n        this.isSub = bool;\r\n        if(this.isSub){\r\n        this.name = arr[8];\r\n        } else{\r\n          this.name = arr[2]; \r\n        }\r\n        this.itemList=[];\r\n        this.subHierNames=[];\r\n        this.subHierList= new Map();\r\n        if(arr[4] == \"NULL\"){\r\n            this.itemList.push(new Item(arr));\r\n        } else if (this.isSub){\r\n            this.itemList.push(new Item(arr));\r\n        } else {\r\n            this.subHierList.set(arr[8], new Hierarchy(arr, true));\r\n            this.subHierNames.push(this.subHierList.get(arr[8]));\r\n        }\r\n        this.bText=this.name+\"Tog\";\r\n    }\r\n\r\n    getName() {\r\n        return this.name;\r\n    }\r\n\r\n    addItem(arr=[]) {\r\n        if(arr[4] == \"NULL\"){\r\n            this.itemList.push(new Item(arr))\r\n        } else if (this.subHierList.has(arr[8])){\r\n            this.subHierList.get(arr[8]).itemList.push(new Item(arr))\r\n        } else {\r\n            this.subHierList.set(arr[8], new Hierarchy(arr, true))\r\n            this.subHierNames.push(this.subHierList.get(arr[8]));\r\n        }\r\n    }\r\n\r\n    ShowHideCategory = () => {\r\n        var x = document.getElementById(this.name);\r\n        if (x.style.display === \"none\"){\r\n            x.style.display = \"block\";\r\n        }\r\n        else {\r\n            x.style.display = \"none\";\r\n        }\r\n        var buttonText=document.getElementById(this.bText);\r\n        if (buttonText.innerText === \"+\"){\r\n            buttonText.innerText=\"-\";\r\n        }\r\n        else {\r\n            buttonText.innerText=\"+\";\r\n        }\r\n    }\r\n\r\n    returnHTML(){\r\n        console.log(this.subHierNames);\r\n        return( \r\n            <div className=\"categoryDiv\">\r\n            <button className=\"categoryButton\" onClick={this.ShowHideCategory} id={this.bText}>-</button>\r\n            {this.name}\r\n            <div id={this.name}>\r\n                {this.itemsToHTML()}\r\n                {this.subHierNames.map((item, i) => \r\n                <div key={i}>{item.returnHTML()}</div>)}\r\n            </div>\r\n            </div>\r\n        )\r\n    }\r\n\r\n    itemsToHTML(){\r\n        return (\r\n            this.itemList.map((item, i) => \r\n            <ul className=\"content-list\" key={i}>\r\n            <li>\r\n                <div>\r\n                    {item.Display}                \r\n                </div>\r\n            </li>\r\n            <li><button className=\"checkBox\" id=\"Mark\">+</button> </li>\r\n            <li>Rating TODO</li>\r\n        </ul>))\r\n    }\r\n\r\n}\r\n\r\nclass Item{\r\n    constructor(arr) {\r\n        this.RubicID = arr[0]\r\n        this.Display = arr[9]\r\n        this.LocationRt = null\r\n        this.LikertValue = [null, null, null, null, null]\r\n    }\r\n\r\n}\r\n","C:\\Users\\Pete\\Desktop\\CS410_northwestern\\client\\src\\components\\DocumentReader.js",["94"],[],"import DocumentFile from \"./Northwestern.pdf\"\r\nimport CommentB from './CritiqueBox'\r\nimport EditFile from \"./EditFile\"\r\n\r\n\r\nexport default function DocumentReader(){\r\n    return (\r\n        <div className=\"component\">\r\n            {/*<div className=\"docVisual\">\r\n            <object data={DocumentFile} type=\"application/pdf\" width=\"100%\" height=\"100%\"></object>\r\n    </div> */}\r\n    <EditFile />\r\n    <br/>\r\n    <br/>\r\n            <CommentB />\r\n        </div>\r\n    )\r\n}","C:\\Users\\Pete\\Desktop\\CS410_northwestern\\client\\src\\components\\CritiqueBox.js",[],[],"C:\\Users\\Pete\\Desktop\\CS410_northwestern\\client\\src\\components\\CommentBox.js",[],[],"C:\\Users\\Pete\\Desktop\\CS410_northwestern\\client\\src\\components\\EditFile.js",[],[],{"ruleId":"95","severity":1,"message":"96","line":14,"column":13,"nodeType":"97","endLine":16,"endColumn":52},{"ruleId":"98","severity":1,"message":"99","line":8,"column":9,"nodeType":"100","messageId":"101","endLine":8,"endColumn":16},{"ruleId":"98","severity":1,"message":"102","line":7,"column":12,"nodeType":"100","messageId":"101","endLine":7,"endColumn":23},{"ruleId":"98","severity":1,"message":"103","line":4,"column":8,"nodeType":"100","messageId":"101","endLine":4,"endColumn":16},{"ruleId":"104","severity":1,"message":"105","line":20,"column":17,"nodeType":"106","messageId"